@import url('https://fonts.googleapis.com/css2?family=Open+Sans&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Dancing+Script:wght@600&display=swap');

*{
margin: 0;
padding: 0;
	
	
	
	
}

.dp-menu ul{
list-style-type:none;
padding: 0;
margin-left:33%;
margin-top:2%;	

}	

.dp-menu ul li{
	
display: inline;	
position:relative;	
	
}	


.dp-menu ul li a{
	
display: inline-block;	
color:	#20B2AA;	
text-decoration:none;
font-size:17px;
 font-family: 'Open Sans', sans-serif;
padding:2px; 
}

.dp-menu ul ul{
	
display:none;
position:absolute;	
left:0;	
	
}



.dp-menu ul li:hover ul{
	
display: block;	

	
	
}	

.dp-menu ul ul{
width: 150px;	
}



.dp-menu ul ul li a{
	
display:block;

	
	
}


