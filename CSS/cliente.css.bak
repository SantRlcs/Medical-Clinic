}
form input[type=text], 
input[type=checkbox],
input[type=password]{
	margin-top: 2%;
	background-color: #F0F8FF;
	border-radius: 5px;
	border: 1px solid #ccc;
	margin-left: 2%;
	height:35px;
}
#options{
	font-family: times new roman;
	background-color: #F0F8FF;
	font-size: 15.7px;
	color: #696969;
	margin-top: 1.8%;
	margin-left: 2%;
	height: 35px;
	width: 95.3%;
	border:  1px solid #ccc;
	border-radius: 5px;
}
form input[type=submit]{
	font-family: times new roman;
	font-size: 18px;
	width: 40%;
	height: 29px;
	background-color:#FF4500;
	color: #ffffff;
	border-radius: 5px;
	border: 1.6px solid #ffffff;
}
form input[type=submit]:hover{
	transition: 1.2s;
	background-color: #20B2AA;
	color: #ffffff;
	border: 1.6px solid #ffffff; 
}
